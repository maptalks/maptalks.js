{
  "name": "maptalks",
  "version": "0.43.0",
  "description": "A light JavaScript library to create integrated 2D/3D maps.",
  "license": "BSD-3-Clause",
  "author": "Fu Zhen",
  "keywords": [
    "javascript",
    "gis",
    "map",
    "2d-maps",
    "3d-maps",
    "geospatial"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maptalks/maptalks.js.git"
  },
  "bugs": {
    "url": "https://github.com/maptalks/maptalks.js/issues"
  },
  "homepage": "https://maptalks.org",
  "scripts": {
    "build": "npm run build-styles && npm run build-dev && gulp minify",
    "build-dev": "rollup -c && gulp transform-proto",
    "build-test": "rollup -c --environment BUILD:test && gulp transform-proto",
    "build-styles": "gulp styles",
    "dev": "rollup -c -w",
    "doc": "gulp doc",
    "lint": "eslint src/**/*.js test/**/*.js",
    "postversion": "gulp zip",
    "pretest": "npm run lint && npm run build-test",
    "prepublish": "npm run build",
    "preversion": "npm run lint",
    "report-coverage": "codecov",
    "test": "gulp test --cov",
    "version": "npm run build && npm run doc && git add -A docs/api"
  },
  "module": "dist/maptalks.es.js",
  "main": "dist/maptalks.js",
  "jsnext:main": "dist/maptalks.es.js",
  "style": "dist/maptalks.css",
  "dependencies": {
    "@maptalks/feature-filter": "^1.0.0",
    "simplify-js": "^1.2.1",
    "zousan": "^2.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-istanbul": "^5.0.1",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-maptalks": "^0.3.0",
    "eslint-plugin-mocha": "^5.0.0",
    "expect-maptalks": "^0.4.1",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "5.0.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-zip": "^4.0.0",
    "happen": "^0.3.1",
    "jsdoc": "^3.5.4",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-expect": "^1.1.3",
    "karma-expect-maptalks": "^0.1.5",
    "karma-firefox-launcher": "^1.1.0",
    "karma-happen": "^0.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "maptalks-jsdoc": "^0.2.4",
    "minimist": "^1.2.0",
    "mocha": "^3.5.0",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "^4.0.3-0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sinon": "^3.2.1",
    "uglify-js": "^3.3.28",
    "zlib": "^1.0.5"
  }
}
