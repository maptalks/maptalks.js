{
  "name": "@maptalks/vt",
  "version": "0.124.3",
  "description": "The vector-tile plugin for maptalks.js",
  "main": "dist/maptalks.vt.js",
  "module": "dist/maptalks.vt.es.js",
  "types": "dist/maptalks.vt.d.ts",
  "files": [
    "dist/maptalks.vt.js",
    "dist/maptalks.vt.js.map",
    "dist/maptalks.vt.es.js",
    "dist/maptalks.vt.es.js.map",
    "dist/maptalks.vt.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.js test/**/*.js",
    "dev": "rollup -w -c build/rollup.config.js",
    "build": "npm run clean && npm run lint && rollup --environment BUILD:production -c build/rollup.config.js",
    "build-dev": "rollup -c build/rollup.config.js",
    "test": "electron-mocha -t 8000 --retries 3 --main ../ignore-gpu-blacklist.js --show-window --renderer ./test/**/*.spec.js -g \"\\btext\\b|ciskip\" --invert",
    "tdd": "electron-mocha -t 8000 --main ../ignore-gpu-blacklist.js --interactive --renderer ./test/**/*.spec.js -g",
    "test-gpu": "electron-mocha -t 8000 --preload ./test/integration/gpu.js --show-window --renderer ./test/**/*.spec.js",
    "tdd-gpu": "electron-mocha -t 8000 --preload ./test/integration/gpu.js --interactive --renderer ./test/**/*.spec.js -g",
    "pretest": "npm run build",
    "preversion": "npm run pretest"
  },
  "dependencies": {
    "@mapbox/point-geometry": "^0.1.0",
    "@mapbox/shelf-pack": "^3.1.0",
    "@mapbox/vector-tile": "^1.3.1",
    "@maptalks/feature-filter": "^1.3.0",
    "@maptalks/function-type": "^1.4.1",
    "@maptalks/geojson-bbox": "^1.0.4",
    "@maptalks/geojson-vt": "^3.5.0",
    "@maptalks/tbn-packer": "^1.4.5",
    "@maptalks/vt-plugin": "workspace:*",
    "animation-easings": "^0.1.0",
    "color": "^3.0.0",
    "earcut": "^3.0.1",
    "fast-deep-equal": "^2.0.1",
    "gl-matrix": "^3.4.0",
    "pbf": "^3.2.1",
    "point-in-polygon": "^1.1.0",
    "quickselect": "^1.0.0",
    "rbush": "^3.0.1",
    "robust-predicates": "^2.0.4",
    "tinyqueue": "^2.0.3",
    "vt-pbf": "^3.1.0"
  },
  "devDependencies": {
    "@maptalks/gl": "workspace:*",
    "@maptalks/gpu": "workspace:*",
    "@maptalks/rollup-plugin-glsl-minify": "^0.1.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "electron": "^38.1.2",
    "electron-mocha": "^12.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-mocha": "^10.5.0",
    "express": "^4.17.1",
    "happen": "^0.3.1",
    "maptalks": "workspace:*",
    "mocha": "^10.3.0",
    "pixelmatch": "^4.0.2",
    "rollup": "^4.17.2",
    "rollup-plugin-dts": "^6.1.0"
  },
  "gitHead": "36a6954658f5a3aff0fe84dd79964ee7f1317b06"
}
