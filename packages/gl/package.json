{
  "name": "@maptalks/gl",
  "version": "0.97.4",
  "description": "WebGL infrastrcture for maptalks",
  "main": "dist/maptalksgl.js",
  "module": "dist/maptalksgl.es.js",
  "scripts": {
    "build": "npm run lint && cd ../reshader.gl && npm run build && cd ../gl && rollup --environment BUILD:production -c rollup.config.js",
    "build-dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -c rollup.config.js",
    "dev": "cd ../reshader.gl && npm run build-dev && cd ../gl && rollup -w -c rollup.config.js",
    "test": "cd ../layer-gltf && npm run build && cd ../gl && karma start --single-run",
    "tdd": "karma start karma.conf.dev.js --no-single-run",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.js",
    "pretest": "cd ../analysis && npm run build && npm run lint && npm run build",
    "preversion": "npm test"
  },
  "files": [
    "dist/maptalksgl.js",
    "dist/maptalksgl.es.js",
    "dist/transcoders.js"
  ],
  "keywords": [
    "webgl",
    "maptalks"
  ],
  "author": "fuzhenn",
  "license": "UNLICENSED",
  "dependencies": {
    "@maptalks/fusiongl": "^0.6.13",
    "@maptalks/gltf-loader": "^0.97.2",
    "@maptalks/martini": "^0.4.0",
    "@maptalks/regl": "^3.3.5",
    "@maptalks/reshader.gl": "^0.97.4",
    "color": "^3.0.0",
    "gl-matrix": "^2.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "eslint": "^8.46.0",
    "eslint-plugin-mocha": "^10.1.0",
    "expect.js": "^0.3.1",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "maptalks": "^1.0.0-rc.28",
    "mocha": "^9.0.3",
    "pixelmatch": "^4.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-terser": "^5.3.1"
  },
  "gitHead": "9eeea4807723d460fa7e09969c6556d53f6bed84"
}
